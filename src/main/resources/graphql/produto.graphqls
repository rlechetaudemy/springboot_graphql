type Produto {
    id: ID!
    nome: String!,
    valor: Float!,
    valorReais:String!
}

input ProdutoInput {
    id:ID,
    nome:String!
    valor: Float,
}

extend type Query {
    produto(id:ID!):Produto

    produtos:[Produto]!
}

extend type Mutation {

    saveProduto(produto:ProdutoInput!):Produto!

    deleteProduto(id:ID!):Boolean!
}